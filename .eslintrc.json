{
	"env": {
		"browser": true,
		"node": true
	},
	"extends": [ "plugin:@wordpress/eslint-plugin/recommended" ],
	"parserOptions": {
		"requireConfigFile": false
	},
	"settings": {
		"react": {
			"version": "18.0"
		}
	},
	"globals": {
		"wp": true,
		"ajaxurl": true,
		"DTN_POSTS": true,
		"jQuery": true
	},
	"rules": {
		"import/no-unresolved": "off",
		"jsdoc/require-param-type": "off",
		"no-duplicate-imports": "off",
		"no-unused-vars": "warn",
		"no-console": "warn",
		"no-alert": "error",
		"vars-on-top": "warn"
	}
}
